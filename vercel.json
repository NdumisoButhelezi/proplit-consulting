{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "rewrites": [
      {
        "source": "/(?!.*\\.(js|css|png|jpg|svg|json)$)",
        "destination": "/index.html"
      }
    ]
  }
  